{
    "name": "thore_e2e_tests",
    "version": "1.0.0",
    "description": "E2E tests for VechainThor protocol",
    "repository": "https://github.com/vechain/thor",
    "license": "MIT",
    "scripts": {
        "postinstall": "hardhat compile",
        "docker:up": "docker compose -f network/docker-compose-solo.yaml up -d --wait",
        "docker:down": "docker compose -f network/docker-compose-solo.yaml down",
        "docker:restart": "yarn docker:down && yarn docker:up",
        "pretest": "yarn docker:restart",
        "test": "yarn jest --forceExit",
        "posttest": "yarn docker:down",
        "test:fast": "yarn jest --forceExit",
        "generate:openapi": "yarn openapi-typescript -o ./src/open-api-types.ts",
        "generate:accounts": "ts-node scripts/generate-genesis-accounts.ts",
        "format": "prettier . --write",
        "lint": "eslint . "
    },
    "devDependencies": {
        "@babel/preset-env": "^7.23.8",
        "@babel/preset-typescript": "^7.23.3",
        "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
        "@nomicfoundation/hardhat-ethers": "^3.0.0",
        "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
        "@nomicfoundation/hardhat-toolbox": "^4.0.0",
        "@nomicfoundation/hardhat-verify": "^2.0.0",
        "@nomiclabs/hardhat-truffle5": "^2.0.7",
        "@nomiclabs/hardhat-web3": "^2.0.0",
        "@typechain/ethers-v6": "^0.5.0",
        "@typechain/hardhat": "^9.0.0",
        "@types/jest": "^29.5.11",
        "@types/node": ">=16.0.0",
        "@types/ws": "^8.5.10",
        "@typescript-eslint/eslint-plugin": "^6.19.0",
        "@typescript-eslint/parser": "^6.19.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "ethers": "^6.4.0",
        "hardhat": "^2.14.0",
        "hardhat-gas-reporter": "^1.0.8",
        "hardhat-jest": "^1.0.8",
        "jest": "^29.7.0",
        "jest-expect-message": "^1.1.3",
        "jest-html-reporters": "^3.1.7",
        "jest-json-result": "^1.0.0",
        "jest-junit": "^16.0.0",
        "openapi-typescript": "^6.7.3",
        "prettier": "^3.2.2",
        "prettier-eslint": "^16.2.0",
        "solidity-coverage": "^0.8.0",
        "ts-node": ">=8.0.0",
        "typechain": "^8.3.0",
        "typescript": "^5.3.3",
        "yarn": "^1.22.21"
    },
    "dependencies": {
        "@openzeppelin/contracts": "^5.0.2",
        "@types/proper-lockfile": "^4.1.4",
        "@vechain/sdk-core": "1.0.0-beta.3",
        "@vechain/sdk-hardhat-plugin": "1.0.0-beta.3",
        "@vechain/sdk-logging": "1.0.0-beta.3",
        "@vechain/sdk-network": "1.0.0-beta.3",
        "async-mutex": "^0.4.0",
        "axios": "^1.6.5",
        "bignumber.js": "^9.1.2",
        "dotenv": "^16.4.5",
        "jest-extended": "^4.0.2",
        "jest-runner-groups": "^2.2.0",
        "mocha-junit-reporter": "^2.2.1",
        "prettier-plugin-solidity": "^1.3.1",
        "proper-lockfile": "^4.1.2",
        "ws": "^8.16.0"
    }
}
