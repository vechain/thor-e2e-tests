        "@nomicfoundation/ignition-core": "^0.15.4",
        "@openzeppelin/contracts": "^5.0.2",
        "@types/chai": "^4.3.16",
        "@types/proper-lockfile": "^4.1.4",
        "@vechain/sdk-core": "1.0.0-rc.5",
        "@vechain/sdk-logging": "1.0.0-rc.5",
